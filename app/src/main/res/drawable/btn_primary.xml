<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />

            <gradient android:angle="-90" android:startColor="#ff378d73" android:endColor="#ff40a789"  />
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#00894E34" />
            <gradient android:angle="-90" android:startColor="#ff42b092" android:endColor="#ff378d73" />
        </shape>
    </item>
</selector>